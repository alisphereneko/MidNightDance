//▼▼(３ランダム勢力の作成)▼▼
//▼▼【解説】・４つのランダム勢力をそれぞれ設定し作成する。
//▼▼「マスターの入れ替え」「放浪人材の加入」「資金の追加」等を実行。
	setv(@allRandompower, randompowerA)
	addv(@allRandompower, randompowerB)
	addv(@allRandompower, randompowerC)
	addv(@allRandompower, randompowerD)
	addv(@allRandompower, randompowerE)
	while (count(@allRandompower) >= 1) {
		index(@allRandompower, 0, @power)
		subv(@allRandompower, @power)

		storeRoamUnitOfSpot(@allRoamSpot, @allRoamUnit)
		shuffle(@allRoamUnit)
		index(@allRoamUnit, 0, @unit)
		addUnit(@unit, @power)
		storeMasterOfPower(@power, @oldMaster)
		storePlayerUnit(@playerUnit)
		if (@playerUnit == @oldMaster) {
			changePlayer(@unit)
		}
		changeMaster(@unit)
		eraseUnit(@oldMaster)

		pushRand(randPoint)
		if (randPoint >= 0 && randPoint <= 32) {
			set(addRandompowerMember, 1)
		}else if (randPoint >= 33 && randPoint <= 65) {
			set(addRandompowerMember, 2)
		}else if (randPoint >= 66 && randPoint <= 99) {
			set(addRandompowerMember, 3)
		}
		while (addRandompowerMember >= 1) {
			sub(addRandompowerMember, 1)

			storeRoamUnitOfSpot(@allRoamSpot, @allRoamUnit)
			shuffle(@allRoamUnit)
			index(@allRoamUnit, 0, @unit)
			addUnit(@unit, @power)
		}

		pushRand(randPoint)
		if (randPoint >= 0 && randPoint <= 32) {
			addMoney(@power, 0)
		}else if (randPoint >= 33 && randPoint <= 65) {
			addMoney(@power, 2000)
		}else if (randPoint >= 66 && randPoint <= 99) {
			addMoney(@power, 4000)
		}
	}
