//▼▼(【routine(sealDelete)】カウント変動処理)▼▼★
//▼▼【解説】・一時ユニットの持つ一時能力スキル「固有能力封印(seal10〜seal2)」のカウントを進める。
//▼▼もし(seal1)であればそのまま除去。
//▼▼またその他のカウント進行要素を持つスキルの変動も同様に実行する。
//▼▼(※基本的にこのルーチンは消去フェイズではなく個別処理開始タイミングで使用する)
//▼▼※呼び出し前「一時ユニット@unit」「一時ユニット所持スキル@skill」に中身が必要。
event sealDelete
{
		if (has(@skill, seal10) == 1) {
			eraseSkill(@unit, seal10)
			addSkill(@unit, seal9)
		}else if (has(@skill, seal9) == 1) {
			eraseSkill(@unit, seal9)
			addSkill(@unit, seal8)
		}else if (has(@skill, seal8) == 1) {
			eraseSkill(@unit, seal8)
			addSkill(@unit, seal7)
		}else if (has(@skill, seal7) == 1) {
			eraseSkill(@unit, seal7)
			addSkill(@unit, seal6)
		}else if (has(@skill, seal6) == 1) {
			eraseSkill(@unit, seal6)
			addSkill(@unit, seal5)
		}else if (has(@skill, seal5) == 1) {
			eraseSkill(@unit, seal5)
			addSkill(@unit, seal4)
		}else if (has(@skill, seal4) == 1) {
			eraseSkill(@unit, seal4)
			addSkill(@unit, seal3)
		}else if (has(@skill, seal3) == 1) {
			eraseSkill(@unit, seal3)
			addSkill(@unit, seal2)
		}else if (has(@skill, seal2) == 1) {
			eraseSkill(@unit, seal2)
			addSkill(@unit, seal1)
		}else if (has(@skill, seal1) == 1) {
			eraseSkill(@unit, seal1)
		}

		if (has(@skill, elation10) == 1) {
			eraseSkill(@unit, elation10)
			addSkill(@unit, elation9)
		}else if (has(@skill, elation9) == 1) {
			eraseSkill(@unit, elation9)
			addSkill(@unit, elation8)
		}else if (has(@skill, elation8) == 1) {
			eraseSkill(@unit, elation8)
			addSkill(@unit, elation7)
		}else if (has(@skill, elation7) == 1) {
			eraseSkill(@unit, elation7)
			addSkill(@unit, elation6)
		}else if (has(@skill, elation6) == 1) {
			eraseSkill(@unit, elation6)
			addSkill(@unit, elation5)
		}else if (has(@skill, elation5) == 1) {
			eraseSkill(@unit, elation5)
			addSkill(@unit, elation4)
		}else if (has(@skill, elation4) == 1) {
			eraseSkill(@unit, elation4)
			addSkill(@unit, elation3)
		}else if (has(@skill, elation3) == 1) {
			eraseSkill(@unit, elation3)
			addSkill(@unit, elation2)
		}else if (has(@skill, elation2) == 1) {
			eraseSkill(@unit, elation2)
			addSkill(@unit, elation1)
		}else if (has(@skill, elation1) == 1) {
			eraseSkill(@unit, elation1)
		}

		if (has(@skill, metamorphosistime10) == 1) {
			eraseSkill(@unit, metamorphosistime10)
			addSkill(@unit, metamorphosistime9)
		}else if (has(@skill, metamorphosistime9) == 1) {
			eraseSkill(@unit, metamorphosistime9)
			addSkill(@unit, metamorphosistime8)
		}else if (has(@skill, metamorphosistime8) == 1) {
			eraseSkill(@unit, metamorphosistime8)
			addSkill(@unit, metamorphosistime7)
		}else if (has(@skill, metamorphosistime7) == 1) {
			eraseSkill(@unit, metamorphosistime7)
			addSkill(@unit, metamorphosistime6)
		}else if (has(@skill, metamorphosistime6) == 1) {
			eraseSkill(@unit, metamorphosistime6)
			addSkill(@unit, metamorphosistime5)
		}else if (has(@skill, metamorphosistime5) == 1) {
			eraseSkill(@unit, metamorphosistime5)
			addSkill(@unit, metamorphosistime4)
		}else if (has(@skill, metamorphosistime4) == 1) {
			eraseSkill(@unit, metamorphosistime4)
			addSkill(@unit, metamorphosistime3)
		}else if (has(@skill, metamorphosistime3) == 1) {
			eraseSkill(@unit, metamorphosistime3)
			addSkill(@unit, metamorphosistime2)
		}else if (has(@skill, metamorphosistime2) == 1) {
			eraseSkill(@unit, metamorphosistime2)
			addSkill(@unit, metamorphosistime1)
		}else if (has(@skill, metamorphosistime1) == 1) {
			eraseSkill(@unit, metamorphosistime1)

			storeud(SAVEmetamorphosisBeforeClass, @metamorphosisBeforeClass)
			setv(@class, @metamorphosisBeforeClass)
			changeClass(@unit, @class)
			pushLevel(@unit, level)
			setLevel(@unit, 1)
			sub(level, 1)
			addLevel(@unit, level)
			if (level == 0) {
				addLevel(@unit, 1)
				setLevel(@unit, 1)
			}
			storeMemberOfUnit(@unit, @allMember)
			storeAllTalent(@allTalent)
			subv(@allMember, @allTalent)
			while (count(@allMember) >= 1) {
				index(@allMember, 0, @member)
				subv(@allMember, @member)
				changeClass(@member, @class)
				pushLevel(@member, level)
				setLevel(@member, 1)
				sub(level, 1)
				addLevel(@member, level)
				if (level == 0) {
					addLevel(@member, 1)
					setLevel(@member, 1)
				}
			}
		}
}
