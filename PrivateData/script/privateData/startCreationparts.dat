event addmemoryunit
{
	//記録ユニットをaddunitする処理
	addspot(memoryunitspot) hidespot(memoryunitspot)
	storeAllTalent(@allUnit)
	while (count(@allUnit) >= 1) {
		index(@allUnit, 0, @unit)
		subv(@allUnit, @unit)
		setv(@memoryunit,memoryunit_)
		addstr(@memoryunit,@unit)
		addunit(@memoryunit,memoryunitspot)
	}
}