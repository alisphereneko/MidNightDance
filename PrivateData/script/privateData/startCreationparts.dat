		//その他____________________________________________________________________________________________________
		}else if (@unit == honokaMAD) {
			setv(@allObjectiveClass, tank)
			addv(@allObjectiveClass, tank)
			addv(@allObjectiveClass, helicopter)
			addv(@allObjectiveClass, helicopter)
		}else if (@unit == honokaAWAKE) {
			setv(@allObjectiveClass, tank)
			addv(@allObjectiveClass, tank)
			addv(@allObjectiveClass, helicopter)
			addv(@allObjectiveClass, helicopter)
		}

		index(@allObjectiveClass,objective,@ObjectiveClass)
		if (@ObjectiveClass==0) {setv(@ObjectiveClass,empty)}
		setv(@memoryunit,memoryunit_)
		addstr(@memoryunit,@unit)
		addskill(@memoryunit,@ObjectiveClass)
		setstatus(@memoryunit,defense,objective)
	}
}

