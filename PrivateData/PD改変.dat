event PDkaihen
{
	//PrivateDataのpurpose==6挙動のスクリプト書き換え案
	//あまり短くならなかったし処理速度や負荷も改善してるのか分からないという存在意義の不明なスクリプト
	//addstrを使ってかっこつけるためにやりました
	
	//例外
	if (@unit==honokaMAD || @unit==honokaAWAKE) {
		setv(@politicsUnit,b01PoliticsUnit_)
		addstr(@politicsUnit,@unit)
		return()
	}

	//所属系統を取得
	storeskillset(classtypeskillset,@PrivateDatakaihen)
	while (@PrivateDatakaihen!=0) {
		index(@PrivateDatakaihen,0,@politicsUnit) subv(@PrivateDatakaihen,@politicsUnit)
		setv(@PrivateDatakaihen2,memberset_type)
		addstr(@PrivateDatakaihen2,@politicsUnit)
		setv(@PrivateDatakaihen3,@PrivateDatakaihen2)
		storeskillset(@PrivateDatakaihen2,@PrivateDatakaihen2)
		if (has(@PrivateDatakaihen2,@unit)) {
			clear(@PrivateDatakaihen3)
			break()
		}

		setv(@PrivateDatakaihen2,@PrivateDatakaihen3)
		addstr(@PrivateDatakaihen2,_roam)
		storeskillset(@PrivateDatakaihen2,@PrivateDatakaihen2)
		if (has(@PrivateDatakaihen2,@unit)) {
			setv(@PrivateDatakaihen3,_roam)
			break()
		}

		setv(@PrivateDatakaihen2,@PrivateDatakaihen3)
		addstr(@PrivateDatakaihen2,_noname)
		storeskillset(@PrivateDatakaihen2,@PrivateDatakaihen2)
		if (has(@PrivateDatakaihen2,@unit)) {
			setv(@PrivateDatakaihen3,_noname)
			break()
		}

		clear(@PrivateDatakaihen2)
	}
	if (@PrivateDatakaihen2==0) {msg(エラー1) return()}
	
	//所属系統の何番目のユニットかを取得  そのindex番号をPrivateDatakaihennumに代入
	set(PrivateDatakaihennum,0)
	while (PrivateDatakaihennum<count(@PrivateDatakaihen2)) {
		index(@PrivateDatakaihen2,PrivateDatakaihennum,@PrivateDatakaihen)
		if (@PrivateDatakaihen==@unit) {break()}
		add(PrivateDatakaihennum,1)
	}
	if (@PrivateDatakaihen!=@unit) {msg(エラー2) return()}

	//msg(&unit& &PrivateDatakaihennum& &PrivateDatakaihennum2&)

	//上の過程で取得したindex番号に応じて数字をaddstrし、内政ユニット名を作成する
	setv(@PrivateDatakaihen2,politicsnumberskillset)
	addstr(@PrivateDatakaihen2,@PrivateDatakaihen3)
	storeskillset(@PrivateDatakaihen2,@PrivateDatakaihen2)
	index(@PrivateDatakaihen2,PrivateDatakaihennum,@PrivateDatakaihen2)
	addstr(@politicsUnit,@PrivateDatakaihen2)

	addstr(@politicsUnit,oliticsUnit_)
	addstr(@politicsUnit,@unit)
	clear(@PrivateDatakaihen) clear(@PrivateDatakaihen2) clear(@PrivateDatakaihen3)
}

