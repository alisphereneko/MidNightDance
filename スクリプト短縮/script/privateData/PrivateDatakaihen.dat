event privateData
{
	//privateDataの変更案
	//addstrとsetud・storeudを利用してif条件式を大幅に減らし、処理の高速化と
	//スクリプティング作業量の削減を図りました。
	//保存されている各データの書き換えがゲーム中にsetudを使って容易に行えるのも強みです
	//(例えば多系統クラスチェンジ時の指向クラス書き換えは、現在は一度クラスを決めた後に別イベントで
	//上書きする形をとっていますが、クラスチェンジイベント時にSAVEObjectiveClass_(ユニット名)を弄る
	//ことで直接書き換え可能になります。)
	//製作の一助となれば幸いです
	
	//purpose==1,2
	//SAVEbelongingToPower_(ユニット名)の形なのでaddstrで作る
	if (purpose == 1) {
		setv(@udSikibetusi,SAVEbelongingToPower_)
		addstr(@udSikibetusi,@unit)
		setud(@udSikibetusi, @power)
	}else if (purpose == 2) {
		setv(@udSikibetusi,SAVEbelongingToPower_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi,@belongingToPower)

	//purpose==3,4
	//SAVEnumberOfMember_(ユニット名)にnumberOfMemberを保存・取り出し
	//数値変数は直接setudできないため、保存時はnumberOfMemberと同数の要素の文字変数を保存し、
	//取り出し時はpushvを利用してこれを取り出します。
	//以下、数値変数は同様のメソッドで保存・呼び出しを行います。
	}else if (purpose == 3) {
		clear(@temp)
		while (numberOfMember>0) {
			addv(@temp,empty)
			sub(numberOfMember,1)
		}
		setv(@udSikibetusi,SAVEnumberOfMember_)
		addstr(@udSikibetusi,@unit)
		setud(@udSikibetusi, @temp)
		pushv(@temp,numberOfMember)
	}else if (purpose == 4) {
		setv(@udSikibetusi,SAVEnumberOfMember_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi, @temp)
		pushv(@temp,numberOfMember)

	//pusrpose==5
	//SAVEObjectiveClass_(ユニット名)から指向クラスを、SAVEObjectiveNumber_(ユニット名)からObjectiveNumberを
	//取り出します。
	}else if (purpose == 5) {
		setv(@udSikibetusi,SAVEObjectiveClass_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi,@allObjectiveClass)
		
		setv(@udSikibetusi,SAVEObjectiveNumber_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi,@temp)
		pushv(@temp,objectiveNumber)
		
		index(@allObjectiveClass,objectiveNumber,@ObjectiveClass)
		
	//purpose==6
	//SAVEpoliticsUnit_(ユニット名)から内政ユニットを取り出します
	}else if (purpose == 6) {
		setv(@udSikibetusi,SAVEpoliticsUnit_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi,@politicsUnit)
		
	//purpose==7
	//SAVEstartClass_(ユニット名)から開始クラスを、SAVEstartLv_(ユニット名)から開始レベルを
	//取り出します。
	}else if (purpose == 7) {
		setv(@udSikibetusi,SAVEstartClass_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi,@startClass)
		
		setv(@udSikibetusi,SAVEstartLv_)
		addstr(@udSikibetusi,@unit)
		storeud(@udSikibetusi,@temp)
		pushv(@temp,startLv)
	}
}

