	storeLeaderOfSpot(invisible9, @invisible9Capa)
	storeLeaderOfSpot(invisible10, @invisible10Capa)
	storeLeaderOfSpot(invisible11, @invisible11Capa)
	storeLeaderOfSpot(invisible12, @invisible12Capa)
	storeLeaderOfSpot(invisible13, @invisible13Capa)
	storeLeaderOfSpot(invisible14, @invisible14Capa)
	storeLeaderOfSpot(invisible15, @invisible15Capa)
	storeLeaderOfSpot(invisible16, @invisible16Capa)
	storeLeaderOfSpot(invisible17, @invisible17Capa)
	storeLeaderOfSpot(invisible18, @invisible18Capa)
	storeLeaderOfSpot(invisible19, @invisible19Capa)
	storeLeaderOfSpot(invisible20, @invisible20Capa)

	if (count(@invisible1Capa) <= 19) {
		addUnit(@unit, invisible1)
	}else if (count(@invisible2Capa) <= 19) {
		addUnit(@unit, invisible2)
	}else if (count(@invisible3Capa) <= 19) {
		addUnit(@unit, invisible3)
	}else if (count(@invisible4Capa) <= 19) {
		addUnit(@unit, invisible4)
	}else if (count(@invisible5Capa) <= 19) {
		addUnit(@unit, invisible5)
	}else if (count(@invisible6Capa) <= 19) {
		addUnit(@unit, invisible6)
	}else if (count(@invisible7Capa) <= 19) {
		addUnit(@unit, invisible7)
	}else if (count(@invisible8Capa) <= 19) {
		addUnit(@unit, invisible8)
	}else if (count(@invisible9Capa) <= 19) {
		addUnit(@unit, invisible9)
	}else if (count(@invisible10Capa) <= 19) {
		addUnit(@unit, invisible10)
	}else if (count(@invisible11Capa) <= 19) {
		addUnit(@unit, invisible11)
	}else if (count(@invisible12Capa) <= 19) {
		addUnit(@unit, invisible12)
	}else if (count(@invisible13Capa) <= 19) {
		addUnit(@unit, invisible13)
	}else if (count(@invisible14Capa) <= 19) {
		addUnit(@unit, invisible14)
	}else if (count(@invisible15Capa) <= 19) {
		addUnit(@unit, invisible15)
	}else if (count(@invisible16Capa) <= 19) {
		addUnit(@unit, invisible16)
	}else if (count(@invisible17Capa) <= 19) {
		addUnit(@unit, invisible17)
	}else if (count(@invisible18Capa) <= 19) {
		addUnit(@unit, invisible18)
	}else if (count(@invisible19Capa) <= 19) {
		addUnit(@unit, invisible19)
	}else if (count(@invisible20Capa) <= 19) {
		addUnit(@unit, invisible20)
// 	}else {
// 		msg(［休憩中ユニットが多すぎるため&unit&は休暇出来ませんでした。$忠誠値が回復し、引き続き戦場に残存します］)
// 		addLoyal(@unit, 100)
// 		eraseSkill(@unit, unitsleeping7)
	}
}
